{
  "name": "bangerrequest-mio",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "ci": "npm run lint && npm run type-check && npm run test && npm run build",
    "health": "node scripts/check_health.js",
    "clean": "rm -rf .next node_modules/.cache",
    "update-deps": "npm update && npm audit fix",
    "telegram:setup": "node scripts/telegram-setup.mjs",
    "telegram:webhook": "node scripts/set-webhook.mjs"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": "22.x",
    "npm": ">=9.8.0"
  }
}
